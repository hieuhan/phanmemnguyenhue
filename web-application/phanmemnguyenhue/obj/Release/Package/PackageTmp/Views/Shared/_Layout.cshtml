<!doctype html>
@using phanmemnguyenhue.Services.Extensions
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Hệ thống quản trị phần mềm Nguyễn Huệ</title>
    <link href="/assets/css/phanmemnguyenhue.min.css" rel="stylesheet" />
    <link href="/assets/libs/select2/select2.min.css" rel="stylesheet" />
    <link href="/assets/libs/toastr/toastr.css" rel="stylesheet" />
    <link href="/assets/css/app.css" rel="stylesheet" />
</head>
<body class="layout-fluid">
    <div class="page">
        @Html.Action("PartialHeader", "Shared")
        <div class="page-wrapper">
            @RenderBody()
            @Html.Partial("PartialFooter")
        </div>
    </div>
    <script src="/assets/libs/jquery/jquery.min.js" defer></script>
    <script src="/assets/js/phanmemnguyenhue.min.js" defer></script>
    <script src="/assets/libs/jquery-validate/jquery.validate.js" defer></script>
    <script src="/assets/libs/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js" defer></script>
    <script src="/assets/libs/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" defer></script>
    <script src="/assets/js/init.min.js" defer></script>
    <script src="/assets/libs/select2/select2.min.js" defer></script>
    <script src="/assets/libs/toastr/toastr.js" defer></script>
    <script src="~/assets/libs/lazyload/lazyload.min.js" defer></script>
    <script src="/assets/js/app.js" defer></script>
    @RenderSection("scripts", required: false)
    @Html.ToastrNotifications()
</body>
</html>